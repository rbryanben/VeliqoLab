<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomEditText">
        <attr name="CustomEditTextHint" format="string"/>
        <attr name="CustomEditTextInputType" format="integer" >
            <enum name="text" value="0" />
            <enum name="number" value="1" />
            <enum name="password" value="2" />
            <enum name="phone" value="4" />
            <enum name="email" value="8" />
        </attr>
    </declare-styleable>
    <declare-styleable name="CustomButton">
        <attr name="CustomButtonText" format="string" />
    </declare-styleable>
</resources>